<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Auth List</title>
    <link href="/css/nav.css" rel="stylesheet">
    <link href="/css/auth_list.css" rel="stylesheet">
</head>
<body class="grey-bg">

  <%- include('nav.ejs') %>

  <div class="container">
    <h1>Auth List</h1>
    <div class="auth-list white-bg">
      <% for (let i = 0; i < 글목록.length; i++) { %>
        <div class="list-box">
          <h4><%= 글목록[i].title %></h4>
          <p>글내용임</p>
        </div>
      <% } %>
    </div>
  </div>

  <script src="/js/auth_list.js"></script>
</body>
</html>
